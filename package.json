{
  "name": "todomanager",
  "version": "1.0.0",
  "description": "Todo manager",
  "main": "dist/index.js",
  "bin": {
    "todomanager": "dist/bin/todoManager.js"
  },
  "engines": {
    "node": "8"
  },
  "jest": {
    "testEnvironment": "node",
    "verbose": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/dist/**"
    ],
    "coverageDirectory": "./coverage",
    "coveragePathIgnorePatterns": [
      "./dist/",
      "./node_modules/",
      "./src/bin",
      "./public/assets",
      "webpack.config.babel.js",
      "./coverage"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "scripts": {
    "test": "jest",
    "babel-node": "babel-node",
    "eslint": "eslint",
    "build": "npm run webpack -- -p --env production && babel src/server --out-dir dist --source-map inline",
    "start:dev:server": "babel-node -- src/server/bin/todoManager.js",
    "start:dev:client": "webpack-dev-server",
    "nodemon": "nodemon",
    "webpack": "webpack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VladVes/todo-manager.git"
  },
  "keywords": [
    "todo",
    "manager"
  ],
  "author": "Vladimir Veselov",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/VladVes/todo-manager/issues"
  },
  "homepage": "https://github.com/VladVes/todo-manager#readme",
  "dependencies": {
    "autoprefixer": "^7.2.3",
    "axios": "*",
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-core": "*",
    "babel-loader": "*",
    "babel-polyfill": "*",
    "babel-preset-env": "*",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "*",
    "babel-preset-stage-0": "*",
    "babel-register": "^6.26.0",
    "bootstrap": "^4.0.0",
    "classnames": "^2.2.5",
    "crypto": "^1.0.1",
    "css-loader": "^0.28.7",
    "debug": "*",
    "dotenv": "^4.0.0",
    "email-validator": "^1.1.1",
    "fontawesome": "^4.7.2",
    "html-webpack-plugin": "^2.30.1",
    "jquery": "^3.2.1",
    "jquery-ujs": "^1.2.2",
    "keygrip": "*",
    "koa": "*",
    "koa-bodyparser": "*",
    "koa-flash-simple": "*",
    "koa-generic-session": "*",
    "koa-logger": "*",
    "koa-methodoverride": "*",
    "koa-pug": "3.0.0-2",
    "koa-router": "next",
    "koa-static": "*",
    "koa-webpack": "*",
    "lodash": "^4.17.4",
    "moment": "^2.19.4",
    "mongoose": "^5.0.4",
    "popper.js": "^1.12.9",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.0.9",
    "precss": "^2.0.0",
    "react": "*",
    "react-dom": "*",
    "react-redux": "*",
    "redux": "*",
    "redux-actions": "*",
    "redux-form": "*",
    "redux-thunk": "*",
    "reselect": "^3.0.1",
    "rollbar": "^2.3.2",
    "style-loader": "^0.19.1",
    "sugarcss": "^1.0.3",
    "sugarss": "^1.0.1",
    "webpack": "^3.10.0"
  },
  "devDependencies": {
    "babel-eslint": "*",
    "babel-preset-react": "^6.24.1",
    "enzyme": "*",
    "enzyme-adapter-react-16": "*",
    "eslint": "^4.17.0",
    "eslint-config-airbnb-base": "*",
    "eslint-plugin-flowtype": "*",
    "eslint-plugin-import": "*",
    "eslint-plugin-jest": "*",
    "eslint-plugin-react": "^7.6.1",
    "jest": "^22.2.1",
    "jest-supertest-matchers": "*",
    "nock": "^9.1.0",
    "nodemon": "^1.14.12",
    "supertest": "*",
    "timeout-then": "*",
    "webpack-dev-server": "^2.11.1"
  }
}
